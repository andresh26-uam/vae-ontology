                                                 :NormativeSystemAlignment ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty <https://w3id.org/def/vaeontology#propertyOptimizedAt> ;
                                                   owl:someValuesFrom :NormativeSystemAlignment
                                                 ] ;
