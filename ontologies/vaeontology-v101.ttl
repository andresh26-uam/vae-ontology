###  https://w3id.org/def/vaeontology#AlignmentStatement
vae:AlignmentStatement rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeStatement ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty vae:onEntity ;
                                                                                owl:allValuesFrom vae:Norm
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty vae:onEntity ;
                                                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass vae:Norm
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                                       [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty vae:onEntity ;
                                                                                owl:allValuesFrom vae:Outcome
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty vae:onEntity ;
                                                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass vae:Outcome
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty vae:withNorm ;
                                                            owl:someValuesFrom vae:Norm
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:withValue ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass vae:ValueConcept
                                       ] ;
                       owl:disjointWith vae:AwarenessStatement ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  \"When norms properly reflect values we say that the norms are value-aligned.” + (previously) “The application of norms always implies situation-awareness first.”" ,
                                    "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “… when the outcomes adequately abide by values - possibly because they satisfy entirely the (value-aligned) norms - we say that these outcomes are value-aligned.”" ;
                       rdfs:label "Alignment Statement" ;
                       rdfs:seeAlso vae:AwarenessStatement ;
                       skos:note "References for the specific ways of certifying alignment: [Nieves Montes, Carles Sierra et al. (Gini Index - Semantics Functions, Preferences,  Promotion, Demotion, Optimally aligned norms) https://www.jair.org/index.php/jair/article/view/13487], [Lera Leri et al. (“Moral Value Interpretation”) https://www.iiia.csic.es/media/filer_public/06/0a/060aec7a-2db6-40d7-b3e6-67de4260a065/2022aamaspluralisticvaluealignment.pdf], [Andrés Holgado-Sánchez (Admissibility of sequences of outcomes) https://link.springer.com/chapter/10.1007/978-3-031-43264-4_27]" .


###  https://w3id.org/def/vaeontology#Allowance
vae:Allowance rdf:type owl:Class ;
              rdfs:subClassOf bhv:Tolerance ;
              rdfs:comment "Allowance: a permissible difference; allowing some freedom to move within limits" ,
                           "Term added automatically" ;
              rdfs:label "Allowance" .


###  https://w3id.org/def/vaeontology#ArgumentStatement
vae:ArgumentStatement rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( vae:AgentStatement
                                                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                          owl:onProperty vae:hasPremise ;
                                                                                          owl:allValuesFrom vae:VaeStatement
                                                                                        ]
                                                                                        [ rdf:type owl:Restriction ;
                                                                                          owl:onProperty vae:isBasedOnCriterion ;
                                                                                          owl:allValuesFrom vae:Criterion
                                                                                        ]
                                                                                      ) ;
                                                                   rdf:type owl:Class
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty vae:isProposedByAgent ;
                                                                   owl:someValuesFrom vae:Agent
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty vae:hasClaim ;
                                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass vae:VaeStatement
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf vae:AgentStatement ;
                      rdfs:comment "An ArgumentStatement is a AgentStatment where a claim is made directly characterizing some value or norm related content (VaeStatement), justified by using other statements (premises) and based on some Criterion." ,
                                   "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “One of the big advantages of explicit representations is that outcomes can be explained by the system itself in terms of norms and values. For example, a user can ask why he is blocked on a social media platform and this is then explained in terms of which community rules he broke and possibly what values these rules reflect (i.e. why the norm was adopted)”." ;
                      rdfs:label "Argument Statement" .


###  https://w3id.org/def/vaeontology#AwarenessStatement
vae:AwarenessStatement rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeStatement ,
                                       [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty vae:onEntity ;
                                                                owl:allValuesFrom vae:Agent
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty vae:onEntity ;
                                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass vae:Agent
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty vae:withNorm ;
                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass vae:Norm
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty vae:withValue ;
                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass vae:ValueConcept
                                                       ]
                                                     )
                                       ] ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] “… a system can be given a formal representation of the norms and/or values that determine its behavior or the behavior it expects from its users. In this case we say that the system is norm and/or value-aware.”" ;
                       rdfs:label "Awareness Statement" ;
                       rdfs:seeAlso vae:AlignmentStatement .


