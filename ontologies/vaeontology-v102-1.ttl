            rdfs:subPropertyOf vae:hasAgent ;
            owl:inverseOf vae:isUserOfSystem ;
            rdfs:domain vae:System ;
            rdfs:range vae:User ;
            rdfs:comment "Indicates that the System has the User specified." ;
            rdfs:label "has user" .


