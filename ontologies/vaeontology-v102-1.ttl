                  rdfs:label "Action On State" .


###  https://w3id.org/def/vaeontology#Agent
vae:Agent rdf:type owl:Class ;
          rdfs:subClassOf dul:Agent ,
                          odrl:Party ,
                          vae:VaeConditionedEntity ;
          rdfs:comment "An agent that takes part in a problem of interest, either by commiting to a ValueConcept or by participating in some Outcome." ;
          rdfs:label "Agent" .


###  https://w3id.org/def/vaeontology#AgentStatement
vae:AgentStatement rdf:type owl:Class ;
                   rdfs:subClassOf dul:Description ,
                                   vae:VaeEntity ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty vae:isProposedByAgent ;
                                     owl:someValuesFrom vae:Agent
                                   ] ;
                   rdfs:comment "An AgentStatement is whatever statement proposed by an Agent in some context about the world." ;
                   rdfs:label "Agent Statement" .


###  https://w3id.org/def/vaeontology#AggregationFunction
vae:AggregationFunction rdf:type owl:Class ;
                        rdfs:subClassOf vae:QuantitativeVaeProperty ;
                        rdfs:comment "An aggregation of some other VaeProperty over a set of Outcomes (or Sequence)." ;
                        rdfs:label "Aggregation Function" .


###  https://w3id.org/def/vaeontology#AlignmentSituation
vae:AlignmentSituation rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeSituation ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty dul:satisfies ;
                                         owl:someValuesFrom vae:AlignmentStatement
                                       ] ;
                       owl:disjointWith vae:AwarenessSituation ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  \"When norms properly reflect values we say that the norms are value-aligned.” + (previously) “The application of norms always implies situation-awareness first.”" ,
                                    "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “… when the outcomes adequately abide by values - possibly because they satisfy entirely the (value-aligned) norms - we say that these outcomes are value-aligned.”" ;
                       rdfs:label "Alignment Statement" ;
                       skos:note "References for the specific ways of certifying alignment: [Nieves Montes, Carles Sierra et al. (Gini Index - Semantics Functions, Preferences,  Promotion, Demotion, Optimally aligned norms) https://www.jair.org/index.php/jair/article/view/13487], [Lera Leri et al. (“Moral Value Interpretation”) https://www.iiia.csic.es/media/filer_public/06/0a/060aec7a-2db6-40d7-b3e6-67de4260a065/2022aamaspluralisticvaluealignment.pdf], [Andrés Holgado-Sánchez (Admissibility of sequences of outcomes) https://link.springer.com/chapter/10.1007/978-3-031-43264-4_27]" .


###  https://w3id.org/def/vaeontology#AlignmentStatement
vae:AlignmentStatement rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeStatement ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:interpretationOn ;
                                         owl:someValuesFrom vae:ValueConcept
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:onEntity ;
                                         owl:allValuesFrom [ rdf:type owl:Class ;
                                                             owl:unionOf ( vae:Norm
                                                                           vae:Outcome
                                                                         )
                                                           ]
                                       ] ;
                       owl:disjointWith vae:AwarenessStatement ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  \"When norms properly reflect values we say that the norms are value-aligned.” + (previously) “The application of norms always implies situation-awareness first.”" ,
                                    "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “… when the outcomes adequately abide by values - possibly because they satisfy entirely the (value-aligned) norms - we say that these outcomes are value-aligned.”" ;
                       rdfs:label "Alignment Statement" ;
                       rdfs:seeAlso vae:AwarenessStatement ;
                       skos:note "References for the specific ways of certifying alignment: [Nieves Montes, Carles Sierra et al. (Gini Index - Semantics Functions, Preferences,  Promotion, Demotion, Optimally aligned norms) https://www.jair.org/index.php/jair/article/view/13487], [Lera Leri et al. (“Moral Value Interpretation”) https://www.iiia.csic.es/media/filer_public/06/0a/060aec7a-2db6-40d7-b3e6-67de4260a065/2022aamaspluralisticvaluealignment.pdf], [Andrés Holgado-Sánchez (Admissibility of sequences of outcomes) https://link.springer.com/chapter/10.1007/978-3-031-43264-4_27]" .


###  https://w3id.org/def/vaeontology#ArgumentStatement
vae:ArgumentStatement rdf:type owl:Class ;
                      rdfs:subClassOf vae:AgentStatement ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:hasClaim ;
                                        owl:someValuesFrom vae:VaeStatement
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:isProposedByAgent ;
                                        owl:someValuesFrom vae:Agent
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:hasPremise ;
                                        owl:allValuesFrom vae:VaeStatement
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:isBasedOnCriterion ;
                                        owl:allValuesFrom vae:Criterion
                                      ] ;
                      rdfs:comment "An ArgumentStatement is a AgentStatment where a claim is made directly characterizing some value or norm related content (VaeStatement), justified by using other statements (premises) and based on some Criterion." ,
                                   "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “One of the big advantages of explicit representations is that outcomes can be explained by the system itself in terms of norms and values. For example, a user can ask why he is blocked on a social media platform and this is then explained in terms of which community rules he broke and possibly what values these rules reflect (i.e. why the norm was adopted)”." ;
                      rdfs:label "Argument Statement" .


###  https://w3id.org/def/vaeontology#AwarenessSituation
vae:AwarenessSituation rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeSituation ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty dul:satisfies ;
                                         owl:someValuesFrom vae:AwarenessStatement
                                       ] ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] “… a system can be given a formal representation of the norms and/or values that determine its behavior or the behavior it expects from its users. In this case we say that the system is norm and/or value-aware.”" ;
                       rdfs:label "Awareness Statement" .


###  https://w3id.org/def/vaeontology#AwarenessStatement
vae:AwarenessStatement rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeStatement ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:interpretationOn ;
                                         owl:someValuesFrom [ rdf:type owl:Class ;
                                                              owl:unionOf ( vae:Norm
                                                                            vae:ValueConcept
                                                                          )
                                                            ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:onEntity ;
                                         owl:someValuesFrom vae:Agent
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty vae:onEntity ;
                                         owl:allValuesFrom vae:Agent
                                       ] ;
                       rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] “… a system can be given a formal representation of the norms and/or values that determine its behavior or the behavior it expects from its users. In this case we say that the system is norm and/or value-aware.”" ;
                       rdfs:label "Awareness Statement" ;
                       rdfs:seeAlso vae:AlignmentStatement .


###  https://w3id.org/def/vaeontology#BooleanVaeProperty
vae:BooleanVaeProperty rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeProperty ;
                       rdfs:comment "A VaeProperty taken boolean values." ;
                       rdfs:label "Boolean Vae Property" .


###  https://w3id.org/def/vaeontology#ConceptNode
vae:ConceptNode rdf:type owl:Class ;
                rdfs:subClassOf vae:TaxonomyNode ;
                owl:disjointWith vae:PropertyNode ;
                rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                rdfs:label "Concept Node" ;
                bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#Context
vae:Context rdf:type owl:Class ;
            rdfs:subClassOf dul:Situation ;
            rdfs:comment "A Context is any kind of entity that is underlying of specific events, situations, descriptions and agents. They can be used to describe, for example, the kind of problem or world where a system is deployed, contextualization of statements, etc." ;
            rdfs:label "Context" ;
            rdfs:seeAlso vae:VaeObject ;
            bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" ,
                       "https://arxiv.org/abs/2305.02748" .


###  https://w3id.org/def/vaeontology#Criterion
vae:Criterion rdf:type owl:Class ;
              rdfs:subClassOf dul:Theory ,
                              odrl:Policy ,
                              vae:AgentStatement ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty odrl:action ;
                                owl:someValuesFrom vae:VaeStatement
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty odrl:assignee ;
                                owl:someValuesFrom vae:Agent
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty odrl:assigner ;
                                owl:someValuesFrom vae:User
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty vae:interpretationOn ;
                                owl:someValuesFrom vae:VaeConditioningEntity
                              ] ;
              rdfs:comment """In ArgumentStatements, the fact that specific claims can follow from some premises must be based on some Criterion (this class) proposed by an authority (i.e. the system designer or the researcher) or evolved by the society in some way. 

Criteria link premises and claims and provide constraints for the premises to be true, for the claim to follow from the premises, from the context, etc.""" ;
              rdfs:label "Criterion" ;
              skos:note """[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “One of the big advantages of explicit representations is that outcomes can be explained by the system itself in terms of norms and values. For example, a user can ask why he is blocked on a social media platform and this is then explained in terms of which community rules he broke and possibly what values these rules reflect (i.e. why the norm was adopted)”. 

Criteria are those conditions that identify value-aligned or missaligned behaviour with values or norms in mind.""" .


###  https://w3id.org/def/vaeontology#Equality
vae:Equality rdf:type owl:Class ;
             rdfs:subClassOf bhv:SelfTrascendence ;
             rdfs:comment "Equality is present in most of the VAE papers so far, as it is easy to formalize mathematically." ;
             rdfs:label "Equality" .


###  https://w3id.org/def/vaeontology#FinalInputOutcome
vae:FinalInputOutcome rdf:type owl:Class ;
                      rdfs:subClassOf vae:MultiInputOutcome ;
                      rdfs:comment "The final relation between inputs and outcomes of a Sequence." ;
                      rdfs:label "Final Input Outcome" ;
                      rdfs:seeAlso vae:Sequence ,
                                   "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#Generalization
vae:Generalization rdf:type owl:Class ;
                   rdfs:subClassOf vae:ValueComparisonStatement ;
                   owl:disjointWith vae:ImportanceRelation ;
                   rdfs:comment "A comparison statement proposed by an Agent in which believes that some ValueConcept (superior) generalizes another one (inferior)." ;
                   rdfs:label "Generalization" ;
                   rdfs:seeAlso vae:generalizesNode .


###  https://w3id.org/def/vaeontology#GiniIndex
vae:GiniIndex rdf:type owl:Class ;
              rdfs:subClassOf vae:SemanticsFunction ;
              rdfs:comment "The Gini Index can be used as a Semantics Function in State, as in the taxing problem from Montes and Sierra (see reference)." ;
              rdfs:label "Gini Index" ;
              bhv:bibRef """Montes, N., Sierra, C.: Synthesis and properties of optimally value-aligned nor-
mative systems. Journal of Artificial Intelligence Research 74, 1739–1774 (2022).
https://doi.org/10.1613/jair.1.13487""" .


###  https://w3id.org/def/vaeontology#ImportanceRelation
vae:ImportanceRelation rdf:type owl:Class ;
                       rdfs:subClassOf vae:ValueComparisonStatement ;
                       rdfs:comment "\"Another important concept considered core in the social sciences is value priority, or how important a value is for someone.\"  [Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ,
                                    "A comparison statement proposed by an Agent in which believes that some ValueConcept (superior) is more important than another one (inferior)." ;
                       rdfs:label "Importance Relation" ;
                       rdfs:seeAlso "http://hdl.handle.net/10261/197622" ;
                       bhv:bibRef "Value Systems as a ranking between values: [Serramia et al. https://aamas.csc.liv.ac.uk/Proceedings/aamas2018/pdfs/p1294.pdf]" .


###  https://w3id.org/def/vaeontology#MultiInputOutcome
vae:MultiInputOutcome rdf:type owl:Class ;
                      rdfs:subClassOf vae:Outcome ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:isStartOfSequence ;
                                        owl:allValuesFrom vae:Sequence
                                      ] ;
                      rdfs:comment "A relationship between a series of input dul:Events and a series of Outcomes that follow those. MultiInputOutcomes are the basic blocks to build Sequences, they follow the Sequence ODP." ;
                      rdfs:label "Multi Input Outcome" ;
                      rdfs:seeAlso vae:Sequence ,
                                   "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#NodeImportance
vae:NodeImportance rdf:type owl:Class ;
                   rdfs:subClassOf vae:QuantitativeVaeProperty ,
                                   vae:ValueProperty ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty vae:propertyExpressedAt ;
                                     owl:someValuesFrom vae:TaxonomyNode
                                   ] ;
                   rdfs:comment "\"Another important concept considered core in the social sciences is value priority, or how important a value is for someone. We incorporate this concept by attaching a mea- sure of importance to each node of the taxonomy.\"  [Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                   rdfs:label "Node Importance" ;
                   rdfs:seeAlso vae:ImportanceRelation ,
                                vae:ValueTaxonomy .


###  https://w3id.org/def/vaeontology#Norm
vae:Norm rdf:type owl:Class ;
         rdfs:subClassOf dul:Norm ,
                         odrl:Policy ,
                         vae:VaeConditionedEntity ,
                         vae:VaeConditioningEntity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty odrl:action ;
                           owl:someValuesFrom vae:Outcome
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty odrl:assignee ;
                           owl:someValuesFrom vae:Agent
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty odrl:assigner ;
                           owl:someValuesFrom vae:User
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty odrl:target ;
                           owl:someValuesFrom vae:System
                         ] ;
         owl:disjointWith vae:ValueTaxonomyStatement ;
         rdfs:comment "\"Norms (...) establish boundaries, either soft or hard, on individual autonomy through a variety of mechanisms such as social pressure and expectations, constraints on actions, and sanctions for violation or rewards for compliance.\"" ,
                      "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] \"Explicit representations of norms are often called policies.\"" ;
         rdfs:label "Norm" ;
         bhv:bibRef "Nieves Montes, Nardine Osman, and Carles Sierra, ‘Perspective-dependent value alignment of norms’ VALE workshop, ECAI 2023" .


###  https://w3id.org/def/vaeontology#OptimizedProperty
vae:OptimizedProperty rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty vae:isOptimizationOfProperty ;
                                            owl:someValuesFrom vae:VaeProperty
                                          ] ;
                      rdfs:subClassOf vae:BooleanVaeProperty ,
                                      vae:QuantitativeVaeProperty ;
                      rdfs:comment "Assertion that a property has being optimized over a set of possible outcomes, supplying the value achieved (e.g. an optimal (maximum) vae:GiniIndex value of 0.9 is vae:measuredOnOutcome vae:State, over some other possible states -the optimization domain could be represented somewhere else or with a custom property-)." ;
                      rdfs:label "Optimized Property" .


###  https://w3id.org/def/vaeontology#Outcome
vae:Outcome rdf:type owl:Class ;
            rdfs:subClassOf dul:Event ,
                            vae:VaeConditionedEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty vae:composedBySequence ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass vae:Sequence
                            ] ;
            rdfs:comment "Outcomes include the effect of the behavior of a system or of a user using a system [Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]." ;
            rdfs:label "Outcome" .


###  https://w3id.org/def/vaeontology#Path
vae:Path rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( vae:Sequence
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty vae:hasInputOutcome ;
                                                      owl:allValuesFrom vae:Transition
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf vae:Sequence .


###  https://w3id.org/def/vaeontology#PromotionDemotion
vae:PromotionDemotion rdf:type owl:Class ;
                      rdfs:subClassOf vae:QualitativeVaeProperty ,
                                      vae:ValueProperty ;
                      rdfs:comment "A QualitativeVaeProperty and a ValueProperty that can represent Promotion, Demotion or Neutrality of an Outcome towards some Value." ;
                      rdfs:label "Promotion Demotion" .


###  https://w3id.org/def/vaeontology#PropertyNode
vae:PropertyNode rdf:type owl:Class ;
                 rdfs:subClassOf vae:TaxonomyNode ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty vae:hasPropertyTaxonomy ;
                                   owl:someValuesFrom vae:TaxonomyProperty
                                 ] ;
                 rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                 rdfs:label "Property Node" ;
                 bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#QualitativeVaeProperty
vae:QualitativeVaeProperty rdf:type owl:Class ;
                           rdfs:subClassOf vae:VaeProperty ;
                           rdfs:comment "A VaeProperty that represent a qualitative consideration on an Outcome with respect to a Value or Norm." ;
                           rdfs:label "Qualitative Vae Property" .


###  https://w3id.org/def/vaeontology#QuantitativePromotionDemotion
vae:QuantitativePromotionDemotion rdf:type owl:Class ;
                                  rdfs:subClassOf vae:PromotionDemotion ,
                                                  vae:QuantitativeVaeProperty ;
                                  rdfs:comment "A quantitative version of PromotionDemotion that specifies some degre of Promotion/Demotion/Neutrality." ;
                                  rdfs:label "Quantitative Promotion Demotion" .


###  https://w3id.org/def/vaeontology#QuantitativeVaeProperty
vae:QuantitativeVaeProperty rdf:type owl:Class ;
                            rdfs:subClassOf vae:VaeProperty ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty rdf:value ;
                                              owl:someValuesFrom owl:real
                                            ] ;
                            rdfs:comment "A VaeProperty that is represented by some numeric value." ;
                            rdfs:label "Quantitative Vae Property" .


###  https://w3id.org/def/vaeontology#SemanticsFunction
vae:SemanticsFunction rdf:type owl:Class ;
                      rdfs:subClassOf vae:QuantitativeVaeProperty ,
                                      vae:ValueProperty ;
                      rdfs:comment "\"To express the relationship between an abstract value and the meaning it takes in a particular context, we say that an explicit goal g grounds the semantics of value v in context C\" [Nieves Montes, Carles Sierra et al. https://www.jair.org/index.php/jair/article/view/13487]" ;
                      rdfs:label "Semantics Function" ;
                      bhv:bibRef """Montes, N., Sierra, C.: Synthesis and properties of optimally value-aligned nor-
mative systems. Journal of Artificial Intelligence Research 74, 1739–1774 (2022).
https://doi.org/10.1613/jair.1.13487""" .


###  https://w3id.org/def/vaeontology#Sequence
vae:Sequence rdf:type owl:Class ;
             rdfs:subClassOf vae:Outcome ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty vae:startsWithInputOutcome ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass vae:MultiInputOutcome
                             ] ;
             rdfs:comment "A Sequence of Outcomes. In general, allows representing sequences of input dul:Events and their related Outcomes (e.g. series of states -> actions, newstates -> actions, etc). It follows the Sequence ODP." ;
             rdfs:label "Sequence" ;
             rdfs:seeAlso "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#SoundArgument
vae:SoundArgument rdf:type owl:Class ;
                  rdfs:subClassOf odrl:Operator ;
                  rdfs:comment "Any operator used in a Constraint for a Criterion, between premises and claims contrasting if the argument is sound." ;
                  rdfs:label "Sound Argument" .


###  https://w3id.org/def/vaeontology#State
vae:State rdf:type owl:Class ;
          rdfs:subClassOf vae:Outcome ;
          rdfs:comment "A representation of the State of an Agent." ;
          rdfs:label "State" .


###  https://w3id.org/def/vaeontology#Strategy
vae:Strategy rdf:type owl:Class ;
             rdfs:subClassOf vae:Outcome ;
             rdfs:comment "A mapping between States and Actions that an ActingAgent follows and regulate its behaviour in the world." ;
             rdfs:label "Strategy" .


###  https://w3id.org/def/vaeontology#System
vae:System rdf:type owl:Class ;
           rdfs:subClassOf dul:Collection ,
                           vae:Agent ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty vae:hasTrace ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass vae:Sequence
                           ] ;
           owl:disjointWith vae:User ;
           rdfs:comment "A system is the organization of a set of Agents modelling some complex joint behaviour or for solving a task. A system has a trace, a Sequence of the inputs and the corresponding Outcomes performed (e.g.) by the system or the agents on it." ;
           rdfs:label "System" .


###  https://w3id.org/def/vaeontology#TaxonomyAlignment
vae:TaxonomyAlignment rdf:type owl:Class ;
                      rdfs:subClassOf vae:QuantitativeVaeProperty ,
                                      vae:ValueProperty ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty vae:propertyExpressedAt ;
                                        owl:someValuesFrom vae:ValueTaxonomyStatement
                                      ] ;
                      rdfs:comment "\"We argue that the property-based nodes of a value taxonomy introduce the foundations for linking abstract value concepts to concrete computational constructs that can help formally assess the alignment of behaviour with those values. The value alignment of an entity’s behaviour becomes the degree of satisfaction of the property-based value nodes of the relevant taxonomy that this behaviour brings about.\"" ;
                      rdfs:label "Taxonomy Alignment" ;
                      rdfs:seeAlso vae:ValueTaxonomy ;
                      bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#TaxonomyNode
vae:TaxonomyNode rdf:type owl:Class ;
                 rdfs:subClassOf vae:VaePropertyStatement ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty vae:isTaxonomyNodeOf ;
                                   owl:someValuesFrom vae:ValueTaxonomyStatement
                                 ] ;
                 rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                 rdfs:label "A node of a ValueTaxonomy." ;
                 rdfs:seeAlso vae:ValueTaxonomyStatement ;
                 bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#TaxonomyProperty
vae:TaxonomyProperty rdf:type owl:Class ;
                     rdfs:subClassOf vae:QuantitativeVaeProperty ,
                                     vae:ValueProperty ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty vae:propertyExpressedAt ;
                                       owl:someValuesFrom vae:PropertyNode
                                     ] ;
                     rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                     rdfs:label "Property Taxonomy" .


###  https://w3id.org/def/vaeontology#TextualVaeProperty
vae:TextualVaeProperty rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeProperty ;
                       rdfs:comment "A VaeProperty expressed in natural language." ;
                       rdfs:label "Textual Vae Property" .


###  https://w3id.org/def/vaeontology#Transition
vae:Transition rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( vae:MultiInputOutcome
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vae:hasInput ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass vae:Action
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vae:hasInput ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass vae:State
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty vae:hasOutcome ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass vae:State
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf vae:Outcome .


###  https://w3id.org/def/vaeontology#User
vae:User rdf:type owl:Class ;
         rdfs:subClassOf dul:Person ,
                         vae:Agent ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty vae:isUserOfSystem ;
                           owl:someValuesFrom vae:System
                         ] ;
         rdfs:comment "A User is a human agent that can take part in a system (or model it, supervise it, interact with it...)" ;
         rdfs:label "User" .


###  https://w3id.org/def/vaeontology#VaeConditionedEntity
vae:VaeConditionedEntity rdf:type owl:Class ;
                         rdfs:subClassOf vae:VaeStatementEntity .


###  https://w3id.org/def/vaeontology#VaeConditioningEntity
vae:VaeConditioningEntity rdf:type owl:Class ;
                          rdfs:subClassOf vae:VaeStatementEntity .


###  https://w3id.org/def/vaeontology#VaeEntity
vae:VaeEntity rdf:type owl:Class ;
              rdfs:subClassOf dul:Entity ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dul:hasSetting ;
                                owl:someValuesFrom vae:Context
                              ] ;
              rdfs:comment "Any kind of object from the VAE domain that is contextualized." ;
              rdfs:label "Vae Object" ;
              rdfs:seeAlso vae:Context .


###  https://w3id.org/def/vaeontology#VaeProperty
vae:VaeProperty rdf:type owl:Class ;
                rdfs:subClassOf dul:Description ,
                                vae:VaeStatementEntity ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vae:measuredOnOutcome ;
                                  owl:someValuesFrom vae:Outcome
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vae:propertyExpressedAt ;
                                  owl:allValuesFrom vae:VaePropertyStatement
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty vae:relationWith ;
                                  owl:allValuesFrom vae:VaeConditioningEntity
                                ] ;
                rdfs:comment "Any property measured on an Outcome, useful to describe some aspect that is related to a Norm or a Value" ;
                rdfs:label "Vae Property" .


###  https://w3id.org/def/vaeontology#VaePropertyStatement
vae:VaePropertyStatement rdf:type owl:Class ;
                         rdfs:subClassOf vae:AgentStatement ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty vae:expressesProperty ;
                                           owl:someValuesFrom vae:VaeProperty
                                         ] ;
                         rdfs:comment "An AgentStatement where the agent expresses some VaeProperty of an Outcome." ;
                         rdfs:label "Vae Property Statement" .


###  https://w3id.org/def/vaeontology#VaeSituation
vae:VaeSituation rdf:type owl:Class ;
                 rdfs:subClassOf odrl:Asset ,
                                 vae:VaeEntity ,
                                 schema:FrameOccurrence ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty dul:satisfies ;
                                   owl:someValuesFrom vae:VaeStatement
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty dul:satisfies ;
                                   owl:someValuesFrom vae:ValueConcept
                                 ] ;
                 owl:disjointWith vae:VaeStatement ;
                 rdfs:comment "Any kind of VaePropertyStatement proposed by an agent in which they express that a complex notion of understanding, preservation or respect with a ValueConcept/Norm is present on entity (Agent or Outcome or Norm) based on some VaeProperty(s) seen." ;
                 rdfs:label "Vae Statement" .


###  https://w3id.org/def/vaeontology#VaeStatement
vae:VaeStatement rdf:type owl:Class ;
                 rdfs:subClassOf odrl:Action ,
                                 odrl:Asset ,
                                 vae:VaePropertyStatement ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty vae:interpretationOn ;
                                   owl:someValuesFrom vae:VaeConditioningEntity
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty vae:onEntity ;
                                   owl:someValuesFrom vae:VaeConditionedEntity
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty dul:isSatisfiedBy ;
                                   owl:allValuesFrom vae:VaeSituation
                                 ] ;
                 rdfs:comment "Any kind of VaePropertyStatement proposed by an agent in which they express that a complex notion of understanding, preservation or respect with a ValueConcept/Norm is present on entity (Agent or Outcome or Norm) based on some VaeProperty(s) seen." ;
                 rdfs:label "Vae Statement" .


###  https://w3id.org/def/vaeontology#VaeStatementEntity
vae:VaeStatementEntity rdf:type owl:Class ;
                       rdfs:subClassOf vae:VaeEntity .


###  https://w3id.org/def/vaeontology#Value
vae:Value rdf:type owl:Class ;
          rdfs:subClassOf vae:ValueConcept ;
          rdfs:comment "\"Values are very general, abstract guiding principles that individuals and groups utilise to generate judgements on a variety of constructs, such as actions, strategies, conventions and policies\"" ;
          rdfs:label "Value" ;
          bhv:bibRef "Nieves Montes, Nardine Osman, and Carles Sierra, ‘Perspective-dependent value alignment of norms’ VALE workshop, ECAI 2023" .


###  https://w3id.org/def/vaeontology#ValueComparisonStatement
vae:ValueComparisonStatement rdf:type owl:Class ;
                             rdfs:subClassOf vae:VaePropertyStatement ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty vae:comparisonHasInferior ;
                                               owl:someValuesFrom vae:ValueConcept
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty vae:comparisonHasSuperior ;
                                               owl:someValuesFrom vae:ValueConcept
                                             ] ;
                             rdfs:comment "Any kind of comparison proposed by an Agent (or that the Agent believes in) between two instances of ValueConcepts, under some context." ;
                             rdfs:label "Value Comparison Statement" .


###  https://w3id.org/def/vaeontology#ValueConcept
vae:ValueConcept rdf:type owl:Class ;
                 rdfs:subClassOf vae:VaeConditioningEntity ,
                                 schema:ConceptualFrame ;
                 rdfs:comment "Any abstraction of Value, something that is considered as such even if not present in any specific theory." ;
                 rdfs:label "Value Concept" ;
                 rdfs:seeAlso <http://www.ontologydesignpatterns.org/ont/values/valuecore#Value> .


###  https://w3id.org/def/vaeontology#ValueDefinition
vae:ValueDefinition rdf:type owl:Class ;
                    rdfs:subClassOf vae:AgentStatement ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty schema:perspectiveOn ;
                                      owl:someValuesFrom vae:ValueConcept
                                    ] ;
                    rdfs:comment "A context-dependent grounded definition of a value." ;
                    rdfs:label "Value Definition" .


###  https://w3id.org/def/vaeontology#ValueProperty
vae:ValueProperty rdf:type owl:Class ;
                  rdfs:subClassOf vae:VaeProperty ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty vae:relationWith ;
                                    owl:allValuesFrom vae:ValueConcept
                                  ] ;
                  rdfs:comment "A VaeProperty specifying something about a ValueConcept." ;
                  rdfs:label "Value Property" .


###  https://w3id.org/def/vaeontology#ValueTaxonomyStatement
vae:ValueTaxonomyStatement rdf:type owl:Class ;
                           rdfs:subClassOf vae:VaePropertyStatement ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty vae:hasConceptNode ;
                                             owl:someValuesFrom vae:ConceptNode
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty vae:hasPropertyNode ;
                                             owl:someValuesFrom vae:PropertyNode
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty vae:hasRootNode ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass vae:ConceptNode
                                           ] ;
                           rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                           rdfs:label "Value Taxonomy" ;
                           rdfs:seeAlso vae:TaxonomyAlignment ;
                           bhv:bibRef """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/framester/schema/ConceptualFrame
schema:ConceptualFrame rdf:type owl:Class ;
                       rdfs:subClassOf dul:Description .


###  https://w3id.org/framester/schema/FrameOccurrence
schema:FrameOccurrence rdf:type owl:Class ;
                       rdfs:subClassOf dul:Situation .


###  https://w3id.org/spice/SON/SchwartzValues#Achievement
bhv:Achievement rdf:type owl:Class ;
                rdfs:subClassOf bhv:SelfEnhancement ,
                                bhv:Value ;
                rdfs:label "Achievement" .


###  https://w3id.org/spice/SON/SchwartzValues#Action
bhv:Action rdf:type owl:Class ;
           rdfs:subClassOf bhv:SelfDirection ;
           owl:disjointWith bhv:Thought ;
           bhv:bibRef """\"The freedom to determine one's own actions.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Benevolence
bhv:Benevolence rdf:type owl:Class ;
                rdfs:subClassOf bhv:SelfTrascendence ,
                                bhv:Value ;
                owl:disjointWith bhv:Humility ;
                rdfs:label "Benevolence" .


###  https://w3id.org/spice/SON/SchwartzValues#Caring
bhv:Caring rdf:type owl:Class ;
           rdfs:subClassOf bhv:Benevolence ;
           owl:disjointWith bhv:Dependability ;
           bhv:bibRef """\"Devotion to the wellbeing of group members.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Concern
bhv:Concern rdf:type owl:Class ;
            rdfs:subClassOf bhv:Universalism ;
            bhv:bibRef """\"Commitment to equality, justice and protection for all.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Conformity
bhv:Conformity rdf:type owl:Class ;
               rdfs:subClassOf bhv:Conservation ;
               bhv:bibRef """\"Control of one's own impulses and behaviour, in line with social norms and expectations.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Conservation
bhv:Conservation rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                              owl:onProperty bhv:focus ;
                                                                              owl:someValuesFrom bhv:PersonalFocus
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty bhv:focus ;
                                                                              owl:someValuesFrom bhv:SocialFocus
                                                                            ]
                                                                          )
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty bhv:attitude ;
                                                              owl:allValuesFrom bhv:SelfProtectionAndAnxietyAvoidance
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty bhv:valueMotivation ;
                                                              owl:allValuesFrom bhv:Conservation
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty bhv:opposingValueMotivation ;
                                       owl:someValuesFrom bhv:OpennessToChange
                                     ] ;
                 rdfs:subClassOf vae:ValueConcept .


###  https://w3id.org/spice/SON/SchwartzValues#Dependability
bhv:Dependability rdf:type owl:Class ;
                  rdfs:subClassOf bhv:Benevolence ;
                  bhv:bibRef """\"Being a reliable and trustworthy member of the in-group.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Dominance
bhv:Dominance rdf:type owl:Class ;
              rdfs:subClassOf bhv:Power ;
              owl:disjointWith bhv:Resources ;
              bhv:bibRef """\"Power through exercising control over others.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Face
bhv:Face rdf:type owl:Class ;
         rdfs:subClassOf bhv:Conservation ,
                         bhv:SelfEnhancement ;
         bhv:bibRef """\"Security and power via the keeping up of one's own public image and avoidance of humiliation.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Focus
bhv:Focus rdf:type owl:Class ;
          rdfs:subClassOf vae:ValueConcept .


###  https://w3id.org/spice/SON/SchwartzValues#GrowthAndAnxietyFree
bhv:GrowthAndAnxietyFree rdf:type owl:Class ;
                         rdfs:subClassOf vae:ValueConcept .


###  https://w3id.org/spice/SON/SchwartzValues#Hedonism
bhv:Hedonism rdf:type owl:Class ;
             rdfs:subClassOf bhv:OpennessToChange ,
                             bhv:SelfEnhancement ;
             bhv:bibRef """\"The search for pleasure and sensuous gratification or satisfaction for oneself.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Humility
bhv:Humility rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                          owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                          owl:onProperty bhv:attitude ;
                                                                          owl:someValuesFrom bhv:GrowthAndAnxietyFree
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty bhv:attitude ;
                                                                          owl:someValuesFrom bhv:SelfProtectionAndAnxietyAvoidance
                                                                        ]
                                                                      )
                                                        ]
                                                        [ rdf:type owl:Class ;
                                                          owl:complementOf [ rdf:type owl:Restriction ;
                                                                             owl:onProperty bhv:focus ;
                                                                             owl:someValuesFrom bhv:PersonalFocus
                                                                           ]
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty bhv:focus ;
                                                          owl:allValuesFrom bhv:SocialFocus
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf bhv:Conservation ,
                             bhv:SelfTrascendence ;
             owl:disjointWith bhv:Universalism ;
             bhv:bibRef """\"Acknowledgement of one's insignificance in the grand scheme of things.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#Nature
bhv:Nature rdf:type owl:Class ;
           rdfs:subClassOf bhv:Universalism ;
           bhv:bibRef """\"Preservation of the natural environment.\"

Giménez, August Corrons, and Lluís Garay Tamajón. \"Analysis of the third-order structuring of Shalom Schwartz’s theory of basic human values.\" Heliyon 5.6 (2019): e01797.""" .


###  https://w3id.org/spice/SON/SchwartzValues#OpennessToChange
bhv:OpennessToChange rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty bhv:attitude ;
                                                                  owl:allValuesFrom bhv:GrowthAndAnxietyFree
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty bhv:focus ;
                                                                  owl:allValuesFrom bhv:PersonalFocus
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty bhv:valueMotivation ;
                                                                  owl:allValuesFrom bhv:OpennessToChange
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty bhv:opposingValueMotivation ;
                                           owl:someValuesFrom bhv:Conservation
                                         ] ;
                     rdfs:subClassOf vae:ValueConcept ;
                     rdfs:label "Openness To Change" .


###  https://w3id.org/spice/SON/SchwartzValues#PanCulturalBaselineForValuesPriority
bhv:PanCulturalBaselineForValuesPriority rdf:type owl:Class ;
                                         rdfs:subClassOf vae:ValueConcept ;
